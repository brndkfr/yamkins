<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
  <f:section title="Yammer Notification Configuration">
    <f:entry title="API Token" field="apiToken"
      description="The long-lived API token to access Yammer's REST API.">
      <f:textbox value="${descriptor.geetApiToken()}"/>
    </f:entry>
      <f:entry title="Jenkins Build Server URL" field="buildServerUrl">
          <f:textbox value="${descriptor.getBuildServerUrl()}" />
      </f:entry>
  </f:section>
</j:jelly>
